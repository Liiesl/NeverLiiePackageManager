{
  "comments": {
    "lineComment": "#",
    "blockComment": [null, null]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["`", "`"]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["`", "`"]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*.*[{\\[]\\s*$",
    "decreaseIndentPattern": "^\\s*[}\\]].*$"
  }
}
